<div id="<%= dom_id account %>">
  <p>
    <strong>First name:</strong>
    <%= account.first_name %>
  </p>

  <p>
    <strong>Last name:</strong>
    <%= account.last_name %>
  </p>

  <p>
    <strong>Phone:</strong>
    <%= account.user_phone_number %>
  </p>

  <p>
    <strong>Location:</strong>
    <%= account.user_location %>
  </p>

  <p>
    <strong>About:</strong>
    <%= account.about_user %>
  </p>

  <p>
    <strong>Role:</strong>
    <%= account.user_role %>
  </p>

</div>
